<!-- erstes tab -->
<div id="tabs-1">

	<div class="filter-box filter-usersave">
		<div class="filter-block filter-inline">
			<div class="filter-title">{|Filter|}</div>
			<ul class="filter-list">
				<li class="filter-item">
					<label for="wareneingang" class="switch">
						<input type="checkbox" id="wareneingang">
						<span class="slider round"></span>
					</label>
					<label for="wareneingang">{|Wareneingangsprüfung fehlt|}</label>
				</li>
				<li class="filter-item">
					<label for="rechnung" class="switch">
						<input type="checkbox" id="rechnung" title="alle nicht versendeten Angebote">
						<span class="slider round"></span>
					</label>
					<label for="rechnung">{|Rechnungseingangsprüfung fehlt|}</label>
				</li>
				<li class="filter-item">
					<label for="anhang" class="switch">
						<input type="checkbox" id="anhang">
						<span class="slider round"></span>
					</label>
					<label for="anhang">{|Datei fehlt|}</label>
				</li>
				<li class="filter-item">
					<label for="archiv" class="switch">
						<input type="checkbox" id="archiv">
						<span class="slider round"></span>
					</label>
					<label for="archiv">{|Archiv|}</label>
				</li>
			</ul>
		</div>
	</div>

