<div id="tabs">
  <ul>
    <li><a href="#tabs-1">{|Belege|}</a></li>
    <li><a href="#tabs-2">{|Artikel|}</a></li>
  </ul>
  <div id="tabs-1">
    [MESSAGE]

    <div class="filter-box filter-usersave">
      <div class="filter-block filter-inline">
        <div class="filter-title">{|Filter|}</div>
        <ul class="filter-list">
          <li class="filter-item">
            <label for="closed" class="switch">
              <input type="checkbox" id="closed" name="closed" />
              <span class="slider round"></span>
            </label>
            <label for="closed">{|abgeschlossene|}</label>
          </li>
        </ul>
      </div>
    </div>

    [TAB1]
    [TAB1NEXT]
  </div>
  <div id="tabs-2">
    [MESSAGE]

    <div class="filter-box filter-usersave">
      <div class="filter-block filter-inline">
        <div class="filter-title">{|Filter|}</div>
        <ul class="filter-list">
          <li class="filter-item">
            <label for="closed2" class="switch">
              <input type="checkbox" id="closed2" name="closed2" />
              <span class="slider round"></span>
            </label>
            <label for="closed2">{|abgeschlossene|}</label>
          </li>
        </ul>
      </div>
    </div>

    [TAB2]
  </div>
</div>

<div id="createorderdialog">
  <fieldset><legend>{|Auftrag erstellen|}</legend>
    <form id="frmcreateorderdialog">
      <div id="createorderdialogcontent">
      </div>
      <input type="hidden" id="element" name="element" />
    </form>
  </fieldset>

</div>
<div id="createcreditnotedialog">
  <fieldset><legend>{|Gutschrift erstellen|}</legend>
    <form id="frmcreatecreditnotedialog">
      <div id="createcreditnotedialogcontent">
      </div>
      <input type="hidden" id="celement" name="celement" />
    </form>
  </fieldset>
</div>
